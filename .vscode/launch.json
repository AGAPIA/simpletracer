{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program":"${command:cmake.launchTargetPath}", // "${workspaceRoot}/river.tracer/river.tracer", // 
            "args": ["-p", "libfmi.so", "--annotated", "--z3", "--exprsimplify",  "<", "~/testtools/river/benchmarking-payload/fmi/sampleinput.txt"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [/*
                {
                    "name": "PATH",
                    "value": "$PATH:$HOME/some_path"
                },
                {
                    "name": "OTHER_VALUE",
                    "value": "Something something"
                }*/
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}